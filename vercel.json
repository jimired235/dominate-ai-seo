{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/submit",
      "destination": "/api/submit"
    },
    {
      "source": "/api/leads", 
      "destination": "/api/leads"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    }
  ]
}